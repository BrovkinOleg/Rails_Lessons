
<h2>Test "<%= @test.title %>" has <%= pluralize(@test.questions.count, 'question') %></h2>
<h2>level: <%= @test.level %></h2><br>
<h2>Questions</h2>

<%= link_to 'New question', new_admin_test_question_path(@test) %>

<ol>
